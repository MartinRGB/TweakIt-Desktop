{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true ,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths":{
      "*.ts":["*"],
      "@i18n/*": ["src/components/i18n/*"],
      "@Assets/*": ["src/assets/*"],
      "@Components/*": ["src/components/*"],
      "@Config/*": ["src/config/*"],
      "@Context/*": ["src/context/*"],
      "@Sections/*": ["src/sections/*"],
      "@Styles/*": ["src/styles/*"],
      "@Types": ["src/types"],
    }
  }
}
