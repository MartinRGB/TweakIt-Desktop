{
  "name": "Tweakit",
  "version": "1.0.0",
  "description": "An Electron Tools including TypeScript, React, Jest and ESLint.",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "rm -rf assets/scrcpy-server assets/scrcpy-previewer assets/react-previewer;rm -rf dist;npm-run-all -p build:scrcpy build:main-app build:previewer-app dev:main-app dev:previewer-app electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:main-app": "cross-env NODE_ENV=development webpack-dev-server --config webpack/main-app.webpack.js --mode development",
    "dev:previewer-app": "cross-env NODE_ENV=development webpack-dev-server --config webpack/previewer-app.webpack.js --mode development",
    "electron:serve": "npm-run-all -p main:port && npm run dev:electron",
    "main:port": "wait-on http-get://localhost:50000/",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:main-app",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.js",
    "build:main-app": "webpack --config webpack/main-app.webpack.js",
    "build:previewer-app": "webpack --config webpack/previewer-app.webpack.js",
    "build:scrcpy": "webpack --config webpack/scrcpy.webpack.js",
    "build:scrcpy-standalone": "webpack --config ws-scrcpy-standalone/ws-scrcpy.webpack.js",
    "start:scrcpy-standalone": "npm run build:scrcpy-standalone;node dist/scrcpy-server/index.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir",
    "package-all": "npm run package-mac;npm run package-win;npm run package-linux",
    "test": "jest"
  },
  "keywords": [],
  "author": "MartinRGB <qiuyinsen@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@commitlint/core": "^9.0.1",
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@emotion/core": "^10.0.20",
    "@emotion/styled": "^10.0.17",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/bluebird": "^3.5.33",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^12.0.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.1.0",
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "adbkit": "^2.11.1",
    "autosize": "^4.0.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.7.2",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "cz-conventional-changelog": "3.2.0",
    "d3-ease": "^2.0.0",
    "debug": "^4.3.1",
    "electron": "^8.2.2",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.1",
    "emotion": "^10.0.17",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.2.0",
    "h264-converter": "^0.1.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "promise-socket": "^7.0.0",
    "re-resizable": "^6.9.0",
    "react-draggable": "^4.4.3",
    "react-spring": "^8.0.27",
    "react-svg-pathline": "^0.5.0",
    "react-with-gesture": "^4.0.8",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.0",
    "svg-inline-loader": "^0.8.2",
    "sylvester.js": "^0.1.1",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.0.0-beta.10",
    "theme-ui": "^0.3.3",
    "tinyh264": "0.0.6",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "twin.macro": "^1.12.0",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.1",
    "wait-on": "^5.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^2.5.2",
    "worker-loader": "^3.0.6"
  },
  "dependencies": {
    "@devicefarmer/adbkit": "^2.11.3",
    "i18next": "^19.8.3",
    "node-pty": "^0.9.0",
    "portfinder": "^1.0.28",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.3",
    "styled-components": "^5.1.1",
    "ts-lib": "0.0.5",
    "ws": "^7.4.2"
  },
  "build": {
    "productName": "TweakIt",
    "appId": "com.martinrgb.TweakIt",
    "extraResources": [
      "assets"
    ],
    "mac": {
      "icon": "./icons/macIcon.icns",
      "category": "public.app-category.tools"
    },
    "win": {
      "icon": "./icons/winIcon.ico"
    },
    "linux": {
      "icon": "./icons/linuxIcon"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
