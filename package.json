{
  "name": "Tweakit",
  "version": "1.0.0",
  "description": "An Electron Tools including TypeScript, React, Jest and ESLint.",
  "main": "./dist/main.js",
  "scripts": {
    "dev3": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "dev": "rm -rf assets/scrcpy-server assets/scrcpy-previewer assets/react-previewer;rm -rf dist;npm-run-all -p build:main-app build:previewer-app dev:main-app dev:previewer-app electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:main-app": "cross-env NODE_ENV=development webpack-dev-server --config webpack/main-app.webpack.js --mode development",
    "dev:previewer-app": "cross-env NODE_ENV=development webpack-dev-server --config webpack/previewer-app.webpack.js --mode development",
    "electron:serve": "npm-run-all -p main:port && npm run dev:electron",
    "main:port": "wait-on http-get://localhost:50000/",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:main-app",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.js",
    "build:main-app": "webpack --config webpack/main-app.webpack.js",
    "build:previewer-app": "webpack --config webpack/previewer-app.webpack.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir",
    "package-all": "npm run package-mac;npm run package-win;npm run package-linux",
    "test": "jest"
  },
  "keywords": [],
  "author": "MartinRGB <qiuyinsen@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@commitlint/cli": "^16.0.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/core": "^16.0.0",
    "@electron/remote": "^2.0.1",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/react": "^1.6.22",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@react-three/drei": "^8.6.1",
    "@react-three/fiber": "^7.0.25",
    "@react-three/postprocessing": "^2.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/bluebird": "^3.5.36",
    "@types/electron-devtools-installer": "^2.2.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.20",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "adbkit": "^2.11.1",
    "autosize": "^5.0.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.7.2",
    "commitizen": "^4.2.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cz-conventional-changelog": "3.3.0",
    "d3-ease": "^3.0.1",
    "debug": "^4.3.3",
    "drei": "^2.2.17",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "emotion": "^11.0.0",
    "eslint": "^8.7.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-standard": "^4.1.0",
    "file-loader": "^6.2.0",
    "h264-converter": "^0.1.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postprocessing": "^6.23.5",
    "prettier": "^2.5.1",
    "promise-socket": "^7.0.0",
    "re-resizable": "^6.9.1",
    "react-draggable": "^4.4.4",
    "react-spring": "^9.4.2",
    "react-svg-pathline": "^0.5.0",
    "react-with-gesture": "^4.0.8",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-terser": "^7.0.2",
    "servez": "^1.12.1",
    "stream": "^0.0.2",
    "svg-inline-loader": "^0.8.2",
    "sylvester.js": "^0.1.1",
    "tailwind.macro": "^1.0.0-alpha.10",
    "theme-ui": "^0.13.1",
    "tinyh264": "0.0.7",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "twin.macro": "^2.8.2",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "wait-on": "^6.0.0",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@devicefarmer/adbkit": "^3.2.1",
    "i18next": "^21.6.6",
    "node-pty": "^0.10.1",
    "portfinder": "^1.0.28",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "styled-components": "^5.3.3",
    "ts-lib": "0.0.5",
    "ws": "^8.4.2"
  },
  "build": {
    "productName": "TweakIt",
    "appId": "com.martinrgb.TweakIt",
    "extraResources": [
      "assets"
    ],
    "mac": {
      "icon": "./icons/macIcon.icns",
      "category": "public.app-category.tools"
    },
    "win": {
      "icon": "./icons/winIcon.ico"
    },
    "linux": {
      "icon": "./icons/linuxIcon"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
